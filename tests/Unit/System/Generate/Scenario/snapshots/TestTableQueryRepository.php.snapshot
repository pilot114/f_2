 */
class TestTableQueryRepository extends QueryRepository
{
	protected string $entityName = TestTable::class;


	/**
	 * @return Enumerable<int, TestTable>
	 */
	public function customFind(): Enumerable
	{
		$sql = <<<SQL
		SELECT * FROM $table->name
		SQL;
		return $this->query($sql);
	}
}
